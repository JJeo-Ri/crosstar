      "source": [
        "## [Lv2] BFS, DFS\n",
        "\n",
        " 아래는 BFS 풀이"
      ]
    },
    {
      "cell_type": "code",
      "metadata": {
        "id": "CwmybDcPlLsk"
      },
      "source": [
        "def solution(numbers, target):\n",
        "    uplist = [0]\n",
        "    for n in numbers:\n",
        "        outlist=[]\n",
        "        for m in uplist:\n",
        "            outlist.append(m+n)\n",
        "            outlist.append(m-n)\n",
        "        uplist=outlist\n",
        "    answer = uplist.count(target)\n",
        "    return answer"
      ],
      "execution_count": null,
      "outputs": []
    }
  ]
